<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="tech.soit.typed.example">

    <application
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="tech.soit.typed.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tech.soit.typed.example.EmptyExampleActivity"
            android:label="empty" />
        <activity
            android:name="tech.soit.typed.example.SwipeRefreshActivity"
            android:label="swipe refresh" />
        <activity
            android:name="tech.soit.typed.example.mapping.MappingExampleActivity"
            android:label="mapping" />
    </application>

</manifest>